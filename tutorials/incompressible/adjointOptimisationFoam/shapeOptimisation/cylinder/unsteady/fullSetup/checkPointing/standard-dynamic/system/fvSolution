FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
solvers
{
    "(p|pa).*"
    {
        solver           PCG;
        preconditioner   DIC;
        tolerance        1.e-6;
        relTol           0.1;
    }
    "(p|pa).*Final"
    {
        $p;
        relTol          0;
    }
    "(U|Ua).*"
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        tolerance       5.e-8;
        relTol          0.1;
    }
    "(U|Ua).*Final"
    {
        $U;
        relTol          0;
    }
}

PISO
{
    nCorrectors                 6;
    nNonOrthogonalCorrectors    0;
    pRefCell                    0;
    pRefValue                   0;
    paRefCell	                0;
    paRefValue	                0;

}

